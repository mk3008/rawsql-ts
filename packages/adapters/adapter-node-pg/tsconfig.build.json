{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": ".",
    "paths": {
      "@rawsql-ts/testkit-core": ["../../testkit-core/dist/index"],
      "@rawsql-ts/testkit-postgres": ["../../testkit-postgres/dist/index"],
      "@rawsql-ts/testkit-postgres/*": ["../../testkit-postgres/dist/*"],
      "@rawsql-ts/shared-binder": ["../../_shared/binder/dist"],
      "@rawsql-ts/shared-binder/*": ["../../_shared/binder/dist/*"]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["tests", "node_modules", "dist"]
}
