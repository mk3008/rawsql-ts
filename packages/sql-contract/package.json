{
  "name": "@rawsql-ts/sql-contract",
  "version": "0.1.0",
  "description": "Lightweight mapper and writer helpers that keep client SQL authoritative while exposing an explicit contract for data hydration and CUD emitters.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./mapper": {
      "types": "./dist/mapper/index.d.ts",
      "default": "./dist/mapper/index.js"
    },
    "./mapper/*": {
      "types": "./dist/mapper/*.d.ts",
      "default": "./dist/mapper/*.js"
    },
    "./utils/coercions": {
      "types": "./dist/utils/coercions.d.ts",
      "default": "./dist/utils/coercions.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "default": "./dist/utils/*.js"
    },
    "./writer": {
      "types": "./dist/writer/index.d.ts",
      "default": "./dist/writer/index.js"
    },
    "./writer/*": {
      "types": "./dist/writer/*.d.ts",
      "default": "./dist/writer/*.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:integration": "vitest run --config ../../vitest.integration.config.ts",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "sql",
    "mapper",
    "writer",
    "contract",
    "rawsql"
  ],
  "license": "MIT",
  "author": "msugiura",
  "repository": {
    "type": "git",
    "url": "https://github.com/mk3008/rawsql-ts.git",
    "directory": "packages/sql-contract"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "zod": "^4.3.6",
    "typescript": "^5.8.2",
    "vitest": "^4.0.7",
    "pg-promise": "^12.0.0"
  },
  "files": [
    "dist"
  ]
}
